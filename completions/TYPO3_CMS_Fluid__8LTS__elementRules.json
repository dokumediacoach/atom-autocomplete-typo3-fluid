{
  "elementRules": {
    "localViewHelpers": ["then", "else", "case", "defaultCase"],
    "parent": {
      "if": {
        "firstChild": ["then", "#global"],
        "after": {
          "then": ["else"],
          "else": ["else"]
        }
      },
      "switch": {
        "firstChild": ["case"],
        "after": {
          "case": ["case", "defaultCase"],
          "defaultCase": []
        }
      }
    }
  },
  "meta": {
    "namespace": "TYPO3\\CMS\\Fluid\\ViewHelpers",
    "version": "8LTS",
    "namespacePrefix": "f",
    "xmlns": "http:\/\/typo3.org\/ns\/TYPO3\/CMS\/Fluid\/ViewHelpers"
  }
}
